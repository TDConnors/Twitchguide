@model IEnumerable<ClassProject.Models.Name>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                Name
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Names)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.NameID }) |
                @Html.ActionLink("Details", "Details", new { id=item.NameID }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.NameID })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
